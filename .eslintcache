rolledTop]);\n\n  //====== Manages state for scroll to top button ======\n  useEffect(() => {                                      //adds event listener and sets state currentPos to page current scroll position\n    window.addEventListener('scroll', toggleTopButton, true);\n    return () => {\n      window.removeEventListener('scroll', toggleTopButton,true);\n    }\n  }, [toggleTopButton]);\n  //################################\n\n  return (\n    <div className=\"App\">\n      <NavBar />\n      <Switch>\n        <Route path='/cart'>\n          <Cart />\n        </Route>\n        <Route path='/checkout'>\n          <Checkout />\n        </Route>\n        <Route exact path='/store/products/:SelectedItem' component={Product} />\n        <Route path='/store'>\n          <Store />\n        </Route>\n        <Route path=\"/\">\n          <Home />\n        </Route>\n      </Switch>\n      <Footer />\n      <div className='toTop'\n        style={{ ...style.topButton, opacity: scrolledTop }}>\n        <a href='#top' >\n          <i className=\"fa fa-chevron-up fa-1x insta\" style={style.buttonIcon}></i>\n        </a>\n      </div>\n    </div>\n  );\n}\n\nconst style = {\n  topButton: {\n    backgroundColor: 'white',\n    height: '2rem',\n    width: '2rem',\n    position: 'fixed',\n    bottom: '1.5rem',\n    right: '1rem',\n    borderRadius: '2rem',\n    boxShadow: ' 0px 0px 9px 0px rgba(1,1,1,0.3)',\n    display: 'flex',\n    alignItems: 'center',\n    justifyContent: 'center',\n    zIndex: 100,\n    // border: '2px solid white'\n  },\n  buttonIcon: {\n    color: 'black',\n    position: 'absolute',\n    top: \"calc(50% - 8px)\",\n    right: \"calc(50% - 8px)\"\n  }\n};\n\nexport default App;\n","C:\\Dev\\glossedbyk\\glossedbyk-v3\\src\\Components\\Footer\\Footer.js",[],"C:\\Dev\\glossedbyk\\glossedbyk-v3\\src\\Components\\Carousel (FP)\\CarouselComponent.js",[],"C:\\Dev\\glossedbyk\\glossedbyk-v3\\src\\Components\\NavBar\\NavBarComponent.js",["93"],"import React, { useContex